-- Rotation-Corrected Aura
local char = game.Players.LocalPlayer.Character
local hrp = char:WaitForChild('HumanoidRootPart')

for _, v in pairs(hrp:GetChildren()) do if v.Name == 'SnatchedAura' then v:Destroy() end end

local function create(d)
    local att = Instance.new('Attachment', hrp)
    att.Name = 'SnatchedAura'
    att.CFrame = d.cf -- This applies both Position and Rotation
    local pe = Instance.new('ParticleEmitter', att)
    for prop, val in pairs(d.props) do pcall(function() pe[prop] = val end) end
end

