        SpreadAngle = 360, -360,
        Rotation = NumberRange.new(90.000, 90.000),
        RotSpeed = NumberRange.new(0.000, 0.000),
        Acceleration = Vector3.new(0.000, 34.961, 0.000),
        ZOffset = 3,
        Squash = NumberSequence.new{NumberSequenceKeypoint.new(0.000, 0.000), NumberSequenceKeypoint.new(1.000, 0.000)},
        Shape = Enum.ParticleEmitterShape.Box,
        ShapeInOut = Enum.ParticleEmitterShapeInOut.Outward,
        ShapePartial = 1,
        ShapeStyle = Enum.ParticleEmitterShapeStyle.Volume,
        FlipbookLayout = Enum.ParticleFlipbookLayout.None,
        FlipbookFramerate = NumberRange.new(1.000, 1.000),
        Enabled = false,
        LockedToPart = true,
    }},
}

for _, d in pairs(data) do create(d) end
